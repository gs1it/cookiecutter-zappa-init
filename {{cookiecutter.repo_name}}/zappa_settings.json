{
    "{{cookiecutter.environment}}": {
        "apigateway_enabled": false,
        "app_function": "{{cookiecutter.lambda_handler}}",
        "async_resources": false,
        "aws_region": "{{cookiecutter.aws_region}}",
        "exclude": [
            "*.sh",
            "*.soap",
            "*.zip",
            ".bump2version.cfg",
            ".hg*",
            ".idea",
            ".vscode",
            "IPython",
            "PyYAML*",
            "README.md",
            "Werkzeug*",
            "_*",
            "argcomplete*",
            "boto*",
            "certifi*",
            "cfn_*",
            "chardet*",
            "click*",
            "datashot-imagesave-*",
            "dateutil*",
            "decouple",
            "durationpy*",
            "easy_install.py",
            "fixtures",
            "future*",
            "hjson*",
            "idna*",
            "importlib_*",
            "ipdb*",
            "ipython*",
            "jedi*",
            "jmespath*",
            "kappa*",
            "lib*",
            "package_*",
            "past",
            "pip*",
            "pkg_*",
            "placebo*",
            "python_dateutil*",
            "python_decouple*",
            "python_slugify*",
            "request*",
            "requirements.dev.txt",
            "requirements.txt",
            "run_*",
            "s3transfer*",
            "setup.cfg",
            "setuptools*",
            "six*",
            "slugify*",
            "test*",
            "text_*",
            "toml*",
            "tqdm*",
            "troposphere*",
            "typing*",
            "uploads*",
            "urllib3*",
            "werkzeug*",
            "wheel*",
            "wsgi_*",
            "yaml",
            "zappa*",
            "zipp*"
        ],
        "keep_warm": false,
        "lambda_description": "{{cookiecutter.lambda_description}} - ver. 0.0.1",
        "lambda_handler": "{{cookiecutter.lambda_handler}}",
        "manage_roles": false,
        "memory_size": 128,
        "profile_name": "default",
        "project_name": "{{cookiecutter.project_name}}",
        "role_name": "{{cookiecutter.lambda_role_name}}",
        "runtime": "python{{cookiecutter.python_version}}",
        "s3_bucket": "{{cookiecutter.zappa_s3_bucket}}",
        "timeout_seconds": 10
    }
}